{
    "name": "kkomelin/laravel-translatable-string-exporter",
    "description": "Translatable String Exporter for Laravel",
    "keywords": ["laravel", "translations", "translation", "export", "exporter", "json", "localization"],
    "license": "MIT",
    "authors": [{
        "name": "Konstantin Komelin",
        "email": "konstantin.komelin@gmail.com"
    }],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/support": "^5.4|^6|^7|^8|^9",
        "illuminate/translation": "^5.4|^6|^7|^8|^9",
        "symfony/finder": "^3.2|^4|^5|^6"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0|^2.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0",
        "orchestra/testbench": "^3.4|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "KKomelin\\TranslatableStringExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KKomelin\\TranslatableStringExporter\\Providers\\ExporterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
